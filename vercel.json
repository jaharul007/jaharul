{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/register",
      "destination": "/api/register.js"
    },
    {
      "source": "/api/login",
      "destination": "/api/login.js"
    },
    {
      "source": "/api/user",
      "destination": "/api/user.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
